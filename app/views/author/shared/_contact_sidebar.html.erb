<div id="contact-information" class="col-xs-12">
  <h4>My Contact Information
      <span class="small"><a href=<%= edit_author_author_path(@author) %>>[update]</a></span>
  </h4>

  <p>
    <%= @author.full_name %><br />
    <%= @author.address_1 %><br />
    <%= @author.address_2 %><br />
    <%= @author.city %>, <%= @author.state %> <%= @author.zip %><br />
  </p>

  <p>
    <strong>Phone:</strong> <%= @author.phone_number %> <br />
    <strong>Alternate email:</strong> <%= @author.alternate_email_address %><br />
  </p>
</div>
