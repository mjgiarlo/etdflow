<h1><%= Rails.env %> environment</h1>
<h2> Gem versions for hydra and its dependencies: </h2>
<% @gem_list = [
                'rails',
                'mysql2',
                'hydra',
                'hydra-core',
                'hydra-head',
                'active-fedora',
                'solrizer',
                'blacklight'
               ] %>
<ul>
  <% @gem_list.each do |gem| %>
    <li><%= gem %> version: <%= Gem.loaded_specs["#{gem}"].version %></li>
  <% end %>
</ul>
